{
  "expo": {
    "name": "Lovele",
    "slug": "lovele-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FF6B6B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lovele.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF6B6B"
      },
      "package": "com.lovele.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
